{
  "main": "index.js",
  "scripts": {
    "start": "node node_modules/react-native/local-cli/cli.js start",
    "test": "cross-env-shell jest",
    "ios": "npx pod-install && npx react-native run-ios --simulator='iPhone 12'",
    "ios:device": "npx pod-install && npx react-native run-ios --device",
    "android": "npx react-native run-android --variant=Debug",
    "android:debug": "cd ./android && ./gradlew app:assembleDebug && ./gradlew installDebug",
    "clearcache": "watchman watch-del-all && react-native start --reset-cache",
    "cleangradle": "cd android && ./gradlew clean",
    "build:android:debug": "./gradlew assembleDebug",
    "build:android:release": "./gradlew assembleRelease -x bundleReleaseJsAndAssets",
    "lint": "cross-env-shell tslint --project ./tsconfig.json",
    "lint:fix": "cross-env-shell tslint --fix --project ./tsconfig.json",
    "android:devices": "adb devices",
    "ios:devices": "xcrun instruments -s devices",
    "shake": "adb shell input keyevent 82",
    "podinstall": "npx pod-install",
    "checkTs": "tsc --noEmit"
  },
  "dependencies": {
    "@react-native-community/cameraroll": "^4.0.2",
    "@react-native-community/slider": "^3.0.3",
    "expo": "~40.0.1",
    "expo-splash-screen": "~0.9.0",
    "expo-updates": "~0.4.2",
    "native-base": "^2.15.2",
    "react": "17.0.1",
    "react-dom": "17.0.1",
    "react-native": "~0.63.4",
    "react-native-camera": "^3.42.3",
    "react-native-gesture-handler": "~1.10.2",
    "react-native-reanimated": "~1.13.2",
    "react-native-safe-area-context": "^3.1.9",
    "react-native-screens": "~2.17.1",
    "react-native-unimodules": "~0.12.0",
    "react-native-vector-icons": "^8.0.0",
    "react-native-web": "~0.15.0",
    "react-navigation": "^4.4.4",
    "react-navigation-stack": "^2.10.3",
    "underscore": "^1.12.0"
  },
  "devDependencies": {
    "@babel/core": "~7.13.1",
    "@types/react": "~17.0.2",
    "@types/react-dom": "~17.0.1",
    "@types/react-native": "~0.63.50",
    "@types/react-native-vector-icons": "^6.4.6",
    "babel-preset-expo": "~8.3.0",
    "jest-expo": "~40.0.2",
    "metro-react-native-babel-preset": "^0.65.1",
    "prettier": "^2.2.1",
    "typescript": "~4.1.5"
  },
  "jest": {
    "preset": "react-native"
  },
  "private": true
}
